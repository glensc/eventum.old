{
	"name": "eventum/eventum",
	"description": "Eventum Issue / Bug tracking system",
	"homepage": "https://launchpad.net/eventum",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Elan RuusamÃ¤e",
			"email": "glen@delfi.ee",
			"role": "Developer"
		},
		{
			"name": "Bryan Alsdorf",
			"email": "balsdorf@gmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"irc": "irc://irc.freenode.org/eventum",
		"issues": "https://bugs.launchpad.net/eventum"
	},
	"repositories": [
		{
			"type": "pear",
			"url": "http://pear.php.net",
			"vendor-alias": "pear"
		},
		{
			"type": "package",
			"package": {
				"name": "php-gettext/php-gettext",
				"version": "1.0.11",
				"dist": {
					"url": "https://launchpad.net/php-gettext/trunk/1.0.11/+download/php-gettext-1.0.11.tar.gz",
					"shasum": "9e6f402b460ba5e691e6e21f95d23527ab965f71",
					"type": "tar"
				},
				"autoload": {
					"classmap": [ "gettext.php", "streams.php" ]
				}
			}
		}
	],
	"config": {
		"component-dir": "htdocs/components",
		"component-baseurl": "/components"
	},
	"minimum-stability": "stable",
	"require": {
		"components/jquery": "1.8.*",
		"ext-filter": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-imap": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-mysql": "*",
		"ext-pcre": "*",
		"ext-session": "*",
		"ext-spl": "*",
		"glen/smarty-gettext": "dev-master",
		"pear/Auth_SASL": "*",
		"pear/DB": "*",
		"pear/Date": "*",
		"pear/File_Util": "*",
		"pear/Mail": "*",
		"pear/Mail_Mime": "*",
		"pear/Mail_mimeDecode": "*",
		"pear/Math_Stats": "*",
		"pear/Net_LDAP2": "*",
		"pear/Net_POP3": "*",
		"pear/Net_SMTP": "*",
		"pear/Net_SmartIRC": "*",
		"pear/Net_Socket": "*",
		"pear/Net_URL": "*",
		"pear/PEAR": "*",
		"pear/Text_Diff": "*",
		"pear/XML_RPC": "*",
		"php": ">=5.1.2",
		"php-gettext/php-gettext": "1.0.*",
		"smarty/smarty": "3.1.12"
	},
	"suggest": {
		"ext-ldap": "Adds support for LDAP auth backend",
		"pear/Net_LDAP2": "Adds support for LDAP auth backend"
	},
	"autoload": {
		"files": ["lib/eventum/autoload.php"],
		"classmap": [ "lib/jpgraph/", "lib/sphinxapi/"]
	}
}
